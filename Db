\110\112\117\116\41\10\32\32\32\32\105\102\32\100\114\97\103\103\105\110\103\32\97\110\100\32\105\110\112\117\116\32\61\61\32\100\114\97\103\73\110\112\117\116\32\116\104\101\110\10\32\32\32\32\32\32\32\32\108\111\99\97\108\32\100\101\108\116\97\32\61\32\105\110\112\117\116\46\80\111\115\105\116\105\111\110\32\45\32\100\114\97\103\83\116\97\114\116\10\32\32\32\32\32\32\32\32\70\114\97\109\101\46\80\111\115\105\116\105\111\110\32\61\32\85\68\105\109\50\46\110\101\119\40\10\32\32\32\32\32\32\32\32\32\32\32\32\115\116\97\114\116\80\111\115\46\88\46\83\99\97\108\101\44\10\32\32\32\32\32\32\32\32\32\32\32\32\115\116\97\114\116\80\111\115\46\88\46\79\102\102\115\101\116\32\43\32\100\101\108\116\97\46\88\44\10\32\32\32\32\32\32\32\32\32\32\32\32\115\116\97\114\116\80\111\115\46\89\46\83\99\97\108\101\44\10\32\32\32\32\32\32\32\32\32\32\32\32\115\116\97\114\116\80\111\115\46\89\46\79\102\102\115\101\116\32\43\32\100\101\108\116\97\46\89\10\32\32\32\32\32\32\32\32\41\10\32\32\32\32\101\110\100\10\101\110\100\41\10\10\45\45\32\67\82\69\65\84\69\32\73\78\80\85\84\83\32\65\78\68\32\66\85\84\84\79\78\10\108\111\99\97\108\32\102\117\110\99\116\105\111\110\32\99\114\101\97\116\101\73\110\112\117\116\40\110\97\109\101\44\32\112\108\97\99\101\104\111\108\100\101\114\44\32\112\111\115\105\116\105\111\110\89\41\10\32\32\32\32\108\111\99\97\108\32\105\110\112\117\116\32\61\32\73\110\115\116\97\110\99\101\46\110\101\119\40\34\84\101\120\116\66\111\120\34\41\10\32\32\32\32\105\110\112\117\116\46\83\105\122\101\32\61\32\85\68\105\109\50\46\110\101\119\40\49\44\32\45\50\48\44\32\48\44\32\51\53\41\10\32\32\32\32\105\110\112\117\116\46\80\111\115\105\116\105\111\110\32\61\32\85\68\105\109\50\46\110\101\119\40\48\44\32\49\48\44\32\48\44\32\112\111\115\105\116\105\111\110\89\41\10\32\32\32\32\105\110\112\117\116\46\80\108\97\99\101\104\111\108\100\101\114\84\101\120\116\32\61\32\112\108\97\99\101\104\111\108\100\101\114\10\32\32\32\32\105\110\112\117\116\46\67\108\101\97\114\84\101\120\116\79\110\70\111\99\117\115\32\61\32\102\97\108\115\101\10\32\32\32\32\105\110\112\117\116\46\84\101\120\116\32\61\32\34\34\10\32\32\32\32\105\110\112\117\116\46\66\97\99\107\103\114\111\117\110\100\67\111\108\111\114\51\32\61\32\67\111\108\111\114\51\46\102\114\111\109\82\71\66\40\53\48\44\53\48\44\53\48\41\10\32\32\32\32\105\110\112\117\116\46\84\101\120\116\67\111\108\111\114\51\32\61\32\67\111\108\111\114\51\46\102\114\111\109\82\71\66\40\50\53\53\44\50\53\53\44\50\53\53\41\10\32\32\32\32\105\110\112\117\116\46\66\111\114\100\101\114\83\105\122\101\80\105\120\101\108\32\61\32\48\10\32\32\32\32\105\110\112\117\116\46\80\97\114\101\110\116\32\61\32\70\114\97\109\101\10\32\32\32\32\114\101\116\117\114\110\32\105\110\112\117\116\10\101\110\100\10\10\108\111\99\97\108\32\80\108\97\121\101\114\73\110\112\117\116\32\61\32\99\114\101\97\116\101\73\110\112\117\116\40\34\80\108\97\121\101\114\73\110\112\117\116\34\44\32\34\80\108\97\121\101\114\32\78\97\109\101\34\44\32\49\48\41\10\108\111\99\97\108\32\67\97\110\118\97\115\73\110\112\117\116\32\61\32\99\114\101\97\116\101\73\110\112\117\116\40\34\67\97\110\118\97\115\73\110\112\117\116\34\44\32\34\67\97\110\118\97\115\32\78\117\109\98\101\114\32\40\49\45\49\51\41\34\44\32\53\53\41\10\108\111\99\97\108\32\84\101\120\116\117\114\101\73\110\112\117\116\32\61\32\99\114\101\97\116\101\73\110\112\117\116\40\34\84\101\120\116\117\114\101\73\110\112\117\116\34\44\32\34\84\101\120\116\117\114\101\32\78\117\109\98\101\114\32\40\49\45\53\41\34\44\32\49\48\48\41\10\10\108\111\99\97\108\32\67\108\111\110\101\66\117\116\116\111\110\32\61\32\73\110\115\116\97\110\99\101\46\110\101\119\40\34\84\101\120\116\66\117\116\116\111\110\34\41\10\67\108\111\110\101\66\117\116\116\111\110\46\83\105\122\101\32\61\32\85\68\105\109\50\46\110\101\119\40\49\44\32\45\50\48\44\32\48\44\32\52\48\41\10\67\108\111\110\101\66\117\116\116\111\110\46\80\111\115\105\116\105\111\110\32\61\32\85\68\105\109\50\46\110\101\119\40\48\44\32\49\48\44\32\48\44\32\49\52\53\41\16\101\73\110\112\117\116\40\34\84\101\120\116\117\114\101\73\110\112\117\116\34\44\32\34\84\101\120\116\117\114\101\32\78\117\109\98\101\114\32\40\49\45\53\41\34\44\32\49\48\48\41\10\10\108\111\99\97\108\32\67\108\111\110\101\66\117\116\116\111\110\32\61\32\73\110\115\116\97\110\99\101\46\110\101\119\40\34\84\101\120\116\66\117\116\116\111\110\34\41\10\67\108\111\110\101\66\117\116\116\111\110\46\83\105\122\101\32\61\32\85\68\105\109\50\46\110\101\119\40\49\44\32\45\50\48\44\32\48\44\32\52\48\41\10\67\108\111\110\101\66\117\116\116\111\110\46\80\111\115\105\116\105\111\110\32\61\32\85\68\105\109\50\46\110\101\119\40\48\44\32\49\48\44\32\48\44\32\49\52\53\41\local Players = game:GetService("Players")
local Plots = game:GetService("Workspace").Plots

local SquareSize = 7.231
local LowestX, LowestY = 1.231, 3.231
local UI = Players.LocalPlayer.PlayerGui.MainGui.PaintFrame.GridHolder.Grid

-- FUNCTION TO CLONE GRID
local function CloneGrid(GridObject, Texture, SelectedTexture)
    for _, GridPart in ipairs(GridObject:GetChildren()) do
        if GridPart:IsA("GuiObject") then
            local AbsPos = GridPart.AbsolutePosition
            local AbsSize = GridPart.AbsoluteSize
            local Column = math.round((AbsPos.X - LowestX) / SquareSize)
            local Row = math.round((AbsPos.Y - LowestY) / SquareSize)
            local CellNumber = math.floor((Row * 32) + Column + 1)
            local Rcx = math.round(AbsSize.X / SquareSize) - 1
            local Rcy = math.round(AbsSize.Y / SquareSize) - 1

            local parentCell = UI:FindFirstChild(CellNumber)
            if not parentCell then continue end

            if Texture and SelectedTexture >= 1 and SelectedTexture <= 5 then
                local TextureCopy = GridPart:Clone()
                local scale = 1 + (SelectedTexture - 1) * 0.5
                TextureCopy.Size = UDim2.new(scale, 0, scale, 0)
                TextureCopy.Parent = parentCell

                for rX = 0, Rcx do
                    for rY = 0, Rcy do
                        local NewRow = Row + rY
                        local NewCol = Column + rX
                        local NewCell = math.floor((NewRow * 32) + NewCol + 1)
                        local cloneCell = UI:FindFirstChild(NewCell)
                        if cloneCell then
                            local NewTexture = TextureCopy:Clone()
                            NewTexture.Size = UDim2.new(scale, 0, scale, 0)
                            NewTexture.Parent = cloneCell
                        end
                    end
                end
            else
                parentCell.BackgroundColor3 = GridPart.BackgroundColor3
                for rX = 0, Rcx do
                    for rY = 0, Rcy do
                        local NewRow = Row + rY
                        local NewCol = Column + rX
                        local NewCell = math.floor((NewRow * 32) + NewCol + 1)
                        local cloneCell = UI:FindFirstChild(NewCell)
                        if cloneCell then
                            cloneCell.BackgroundColor3 = GridPart.BackgroundColor3
                        end
                    end
                end
            end
        end
    end
end

-- FUNCTION TO CLONE PLAYER CANVAS
local function ClonePlayerCanvas(PlayerName, CanvasNumber, SelectedTexture)
    local TargetPlayer = Players:FindFirstChild(PlayerName)
    if not TargetPlayer then
        warn("Player not found: " .. tostring(PlayerName))
        return
    end

    local plot = Plots:FindFirstChild(PlayerName)
    if not plot then
        warn("Player plot not found: " .. PlayerName)
        return
    end

    local Easel = plot.Easels:FindFirstChild(tostring(CanvasNumber))
    if not Easel or not Easel:FindFirstChild("Canvas") then
        warn("Canvas not found: " .. tostring(CanvasNumber))
        return
    end

    local ArtGrid = Easel.Canvas.SurfaceGui.Grid
    local ArtTextures = Easel.Canvas.SurfaceGui.TexturesFrame

    CloneGrid(ArtGrid, false, SelectedTexture)
    CloneGrid(ArtTextures, true, SelectedTexture)
end

-- CREATE MOBILE-FRIENDLY CUSTOM UI
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "ArtCloneUI"
ScreenGui.Parent = Players.LocalPlayer:WaitForChild("PlayerGui")

local Frame = Instance.new("Frame")
Frame.Name = "MainFrame"
Frame.Size = UDim2.new(0, 180, 0, 200)
Frame.Position = UDim2.new(0, 20, 0, 50)
Frame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
Frame.BorderSizePixel = 0
Frame.Parent = ScreenGui

-- MAKE FRAME MOVEABLE
local UserInputService = game:GetService("UserInputService")
local dragging, dragInput, dragStart, startPos

Frame.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
        dragging = true
        dragStart = input.Position
        startPos = Frame.Position
        input.Changed:Connect(function()
            if input.UserInputState == Enum.UserInputState.End then
                dragging = false
            end
        end)
    end
end)

Frame.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
        dragInput = input
    end
end)

UserInputService.InputChanged:Connect(function(input)
    if dragging and input == dragInput then
        local delta = input.Position - dragStart
        Frame.Position = UDim2.new(
            startPos.X.Scale,
            startPos.X.Offset + delta.X,
            startPos.Y.Scale,
            startPos.Y.Offset + delta.Y
        )
    end
end)

-- CREATE INPUTS AND BUTTON
local function createInput(name, placeholder, positionY)
    local input = Instance.new("TextBox")
    input.Size = UDim2.new(1, -20, 0, 35)
    input.Position = UDim2.new(0, 10, 0, positionY)
    input.PlaceholderText = placeholder
    input.ClearTextOnFocus = false
    input.Text = ""
    input.BackgroundColor3 = Color3.fromRGB(50,50,50)
    input.TextColor3 = Color3.fromRGB(255,255,255)
    input.BorderSizePixel = 0
    input.Parent = Frame
    return input
end

local PlayerInput = createInput("PlayerInput", "Player Name", 10)
local CanvasInput = createInput("CanvasInput", "Canvas Number (1-13)", 55)
local TextureInput = createInput("TextureInput", "Texture Number (1-5)", 100)

local CloneButton = Instance.new("TextButton")
CloneButton.Size = UDim2.new(1, -20, 0, 40)
CloneButton.Position = UDim2.new(0, 10, 0, 145)
CloneButton.Text = "Clone Art"
CloneButton.BackgroundColor3 = Color3.fromRGB(0, 170, 0)
CloneButton.TextColor3 = Color3.fromRGB(255,255,255)
CloneButton.BorderSizePixel = 0
CloneButton.Parent = Frame

-- BUTTON FUNCTION
CloneButton.MouseButton1Click:Connect(function()
    local playerName = PlayerInput.Text
    local canvasNum = tonumber(CanvasInput.Text) or 1
    local textureNum = tonumber(TextureInput.Text) or 1

    if playerName == "" then
        warn("Enter a valid player name")
        return
    end

    pcall(function()
        ClonePlayerCanvas(playerName, canvasNum, textureNum)
    end)
end)
